package cosmoplas.reglas;

//from row number: 1
rule "Row 1 RuleFirma"
	dialect "mvel"
	when
		f1 : Firma(fechaMovimiento == fechaDespacho )
	then
		f1.setValidacion( true );
end

//from row number: 2
rule "Row 2 RuleFirma"
	dialect "mvel"
	when
		f1 : Firma( fechaMovimiento != fechaDespacho )
	then
		f1.setValidacion( false );
end
